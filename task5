#include <iostream>
#include <math.h>
using namespace std;

int main() {
	setlocale(LC_ALL,"rus");
	bool time,blind,lamp;
	cout<<"На улице светло? Введите 1,если да или 0, если нет"<<endl;
	cin>>time;
	cout<<"Шторы открыты? Введите 1,если да или 0, если нет"<<endl;
	cin>>blind;
	cout<<"Лампа включена? Введите 1,если да или 0, если нет"<<endl;
	cin>>lamp;
	if ((time && blind) or (lamp)) {
		cout<<"light";
	}
	else{
		cout<<"dark";
	}
	return 0;
}
